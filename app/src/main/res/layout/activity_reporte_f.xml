<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ReporteF">

    <Button
        android:id="@+id/btnGenerarF"
        android:layout_width="222dp"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/btnesecundario"
        android:text="Reportar Falla"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.566"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextDesFalla"
        app:layout_constraintVertical_bias="0.045" />

    <EditText
        android:id="@+id/editTextDesFalla"
        android:layout_width="377dp"
        android:layout_height="49dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:layout_weight="9"
        android:background="@drawable/textround"
        android:drawablePadding="10dp"
        android:ems="10"
        android:hint="  Descripción de falla"
        android:inputType="textPersonName"
        android:textColor="#757272"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextDireccion" />

    <EditText
        android:id="@+id/editTextDireccion"
        android:layout_width="377dp"
        android:layout_height="49dp"
        android:layout_marginTop="148dp"
        android:layout_marginEnd="16dp"
        android:layout_weight="9"
        android:background="@drawable/textround"
        android:drawablePadding="10dp"
        android:ems="10"
        android:hint="  Dirección"
        android:inputType="textPersonName"
        android:textColor="#757272"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextNumberC" />

    <EditText
        android:id="@+id/editTextNumberC"
        android:layout_width="377dp"
        android:layout_height="49dp"
        android:layout_marginTop="32dp"
        android:layout_weight="9"
        android:background="@drawable/textround"
        android:drawableLeft="@drawable/number"
        android:ems="10"
        android:hint="Numero de Contrato"
        android:inputType="number"
        android:textColor="#757272"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.529"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <EditText
        android:id="@+id/editTextNombre"
        android:layout_width="377dp"
        android:layout_height="49dp"
        android:layout_marginTop="104dp"
        android:layout_weight="9"
        android:background="@drawable/textround"
        android:ems="10"
        android:hint="  Nombre"
        android:textColor="#757272"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <EditText
        android:id="@+id/editTextApellidos"
        android:layout_width="377dp"
        android:layout_height="49dp"
        android:layout_marginTop="168dp"
        android:layout_weight="9"
        android:background="@drawable/textround"
        android:ems="10"
        android:hint="  Apellidos"
        android:textColor="#757272"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <TextView
        android:id="@+id/textViewReporteF"
        android:layout_width="209dp"
        android:layout_height="44dp"
        android:text="Reporte de Falla"
        android:textColor="#757272"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="198dp"
        android:layout_height="128dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewReporteF"
        app:srcCompat="@drawable/log" />

    <ImageButton
        android:id="@+id/menu4"
        android:layout_width="83dp"
        android:layout_height="78dp"
        android:layout_marginEnd="15dp"
        android:background="@drawable/imgbtn"
        android:onClick="Mas"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.938"
        app:srcCompat="@drawable/mas" />

    <ImageButton
        android:id="@+id/menu3"
        android:layout_width="83dp"
        android:layout_height="78dp"
        android:layout_marginEnd="116dp"
        android:background="@drawable/imgbtn"
        android:onClick="Pag"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.938"
        app:srcCompat="@drawable/pagos" />

    <ImageButton
        android:id="@+id/menu2"
        android:layout_width="83dp"
        android:layout_height="78dp"
        android:layout_marginStart="116dp"
        android:background="@drawable/imgbtn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.938"
        app:srcCompat="@drawable/pag"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/menu1"
        android:layout_width="83dp"
        android:layout_height="78dp"
        android:layout_marginStart="16dp"
        android:background="@drawable/imgbtn"
        android:onClick="Hom"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.938"
        app:srcCompat="@drawable/homeee" />

</android.support.constraint.ConstraintLayout>